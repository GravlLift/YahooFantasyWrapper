<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1</TargetFrameworks>
    <PackageId>YahooFantasyWrapper</PackageId>
    <Title>YahooFantasyWrapper</Title>
    <PackageVersion>1.2.0</PackageVersion>
    <LicenseUrl>https://github.com/isaac2004/YahooFantasyWrapper/blob/master/LICENSE.md</LicenseUrl>
    <ProjectUrl>https://github.com/isaac2004/YahooFantasyWrapper</ProjectUrl>
    <Authors>Isaac Levin</Authors>
    <Description>Wrapper for Yahoo! Fantasy Sports Api</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Added New Models and cleaned up bugs</PackageReleaseNotes>
