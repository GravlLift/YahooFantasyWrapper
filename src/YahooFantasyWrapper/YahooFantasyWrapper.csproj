    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/isaac2004/YahooFantasyWrapper/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/isaac2004/YahooFantasyWrapper/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/isaac2004/YahooFantasyWrapper/blob/master/LICENSE.md</RepositoryUrl>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

